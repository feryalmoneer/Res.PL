@model ServiceVmForm
@section Header{
    Create Services
}
<div class="col-md-8">

    <div class="card card-gray">
      
        <form asp-controller="Services" asp-action="Create" >
            <div class="card-body">
                <div class="form-group">
                    <label for="Name">Service Name</label>
                    <input asp-for="Name" class="form-control" placeholder="Enter email">
                </div>
                <div class="form-group">
                    <label for="Discription">Discription</label>
                    <input asp-for="Discription" class="form-control" placeholder="Discription">
                </div>
               
              
            </div>
            <div class="card-footer">
                <button type="submit" class="btn btn-gray">Create</button>
            </div>
        </form>
    </div>

</div>
